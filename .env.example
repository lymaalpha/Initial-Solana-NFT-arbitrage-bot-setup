# Solana RPC Configuration
# For testing: https://api.devnet.solana.com
# For production: https://api.mainnet-beta.solana.com or Helius/QuickNode
RPC_URL=https://api.devnet.solana.com

# Wallet Configuration (KEEP SECURE!)
# Generate with: solana-keygen new --outfile keypair.json
# Then convert to base58: solana-keygen pubkey keypair.json
PRIVATE_KEY=YOUR_BASE58_ENCODED_PRIVATE_KEY_HERE

# Target NFT Collection
# Example: Bored Ape Yacht Club, Pudgy Penguins, etc.
COLLECTION_MINT=YOUR_TARGET_COLLECTION_MINT_ADDRESS_HERE

# Bot Configuration
SCAN_INTERVAL_MS=5000
MIN_SIGNALS=1
MIN_PROFIT_SOL=0.05
FEE_BUFFER_SOL=0.02

# API Keys (Optional - for better rate limits)
HELIUS_API_KEY=your_helius_api_key_here
MAGIC_EDEN_API_KEY=your_magic_eden_api_key_here

# Logging Configuration
LOG_LEVEL=info
ENABLE_CSV_LOGGING=true
ENABLE_JSON_LOGGING=true

# Alert Configuration (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here
